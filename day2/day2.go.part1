package main

import (
	"fmt"
	"strings"
)

func moveUp(p *int) {
	switch {
	case 1 <= *p && *p <= 3:
	case 4 <= *p && *p <= 9:
		*p -= 3
	default:
		fmt.Printf("Unexpected position %d", *p)
		panic("bad position")
	}
}

func moveDown(p *int) {
	switch {
	case 1 <= *p && *p <= 6:
		*p += 3
	case 4 <= *p && *p <= 9:
	default:
		fmt.Printf("Unexpected position %d", *p)
		panic("bad position")
	}
}

func moveLeft(p *int) {
	switch *p {
	case 1, 4, 7:
	case 2, 3, 5, 6, 8, 9:
		*p--
	default:
		fmt.Printf("Unexpected position %d", *p)
		panic("bad position")
	}
}

func moveRight(p *int) {
	switch *p {
	case 3, 6, 9:
	case 1, 2, 4, 5, 7, 8:
		*p++
	default:
		fmt.Printf("Unexpected position %d", *p)
		panic("bad position")
	}
}

func main() {

	i := `



`

	input := strings.Split(i, "\n")

	var output string
	var pos = 5

	for _, row := range input {
		for _, keypress := range row {
			switch keypress {
			case 'U':
				moveUp(&pos)
			case 'D':
				moveDown(&pos)
			case 'L':
				moveLeft(&pos)
			case 'R':
				moveRight(&pos)
			default:
				fmt.Printf("Unexpected keypress %c", keypress)
				panic("bad keypress")
			}
		}

		output += fmt.Sprintf("%d", pos)
	}

	fmt.Println(output)
}
